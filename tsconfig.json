{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "lib": ["dom", "es2015"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "jsx": "react",
    "baseUrl": "./",
    "sourceMap": true,
    "paths": {
      "@api/*": ["./src/api/*"],
      "@assets/*": ["./src/assets/*"],
      "@bus/*": ["./src/bus/*"],
      "@components/*": ["./src/components/*"],
      "@enum/*": ["./src/enum/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@redux/*": ["./src/redux/*"],
      "@routes/*": ["./src/routes/*"],
      "@ui/*": ["./src/ui/*"]
    }
  },
  "include": ["src", "declarations.d.ts"]
}
